      <script type="text/javascript">
        // comment date
        function daysAgo(x) {
          var dateMinus = parseInt(x);
          var commentDateMinus = new Date();
          commentDateMinus.setDate(commentDateMinus.getDate() - dateMinus);
          var commentMonthMinus = commentDateMinus.getMonth();
          var commentMonthArray = new Array("Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov",
            "Dec");
          var commentDayMinus = commentDateMinus.getDate();
          var commentYearMinus = commentDateMinus.getYear();
          if (commentYearMinus < 1000) {
            commentYearMinus += 1900;
          }
          var commentDaysAgo = commentMonthArray[commentMonthMinus] + " " + commentDayMinus + ", " + commentYearMinus;

          return commentDaysAgo;
        }

        ;
      </script>
